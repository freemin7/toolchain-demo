{
  "yowaspToolchain.bundles": [
    "@yowasp/yosys",
    "@yowasp/nextpnr-ice40",
    "@yowasp/openfpgaloader"
  ],
  "yowaspToolchain.buildCommands": [
    ["yosys", "top.v", "-o", "top.json", "-p", "synth_ice40"],
